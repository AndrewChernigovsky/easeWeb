nav(class=menuClasses[0])
    div(class=menuClasses[1])
        include logo
    div(class=menuClasses[2])
    each ul in menuClasses
        if ul === navUlClasses
            ul(class=ul)
            each href, el in menuLinksUrl
                if el == 1
                    each linkText, el in menuTextNames
                        li(class=menuClasses[4])
                            a(href=href class=menuClasses[5])= linkText    